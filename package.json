    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.6.3",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.0.10",
