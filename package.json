{
  "name": "web-plugin",
  "version": "1.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Electron Vite React boilerplate.",
  "author": "张晗宇",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhy19950705/web-plugin.git"
  },
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
